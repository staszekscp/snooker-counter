{
  "expo": {
    "name": "snooker-counter",
    "slug": "snooker-counter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Ball/6reds-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Ball/6reds.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/Ball/6reds-icon.png"
    }
  }
}
